view,
text,
navigator,
input,
image,
swiper,
swiper-item,
scroll-view,
page {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}
page{
    width: 100%;
    height: 100%;
}
button::after{
    content: "";
    width: 0;
    height: 0;
}
::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
} 